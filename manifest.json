{
  "name": "Telegram Remove Joined",
  "version": "1.0",
  "description": "Chrome Extension: Telegram Web 'joined Telegram' Notification Remover",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://web.telegram.org/*"],
      "js": ["telegram.js"]
    }
  ]
}
